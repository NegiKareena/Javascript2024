{
    "name": "SightRelax",
    "version": "1.0",
    "description": "An extension to help you prevent eye strain and follow the 20-20-20 rule.",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_icon": "./icons/search.png",
      "default_popup": "popup.html",
      "default_title": "SightRelax"
    },
    "permissions": ["tabs", "alarms", "storage", "notifications"]
  }
  